FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 493.15;

boundaryField
{
    rightHand
    {
        type            zeroGradient;
    }
    outerRadius
    {
        type            zeroGradient;
    }
    leftHand
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 493.15;
    }
    innerRadius
    {
        type            zeroGradient;
    }
    oldInternalFaces
    {
        type            empty;
    }
}


