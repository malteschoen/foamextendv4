FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      sink;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    rightHand
    {
        type            zeroGradient;
    }
    outerRadius
    {
        type            zeroGradient;
    }
    leftHand
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    innerRadius
    {
        type            zeroGradient;
    }
    oldInternalFaces
    {
        type            empty;
    }
}


