FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "250";
    object      nu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    rightHand
    {
        type            calculated;
        value           uniform 0.1;
    }
    outerRadius
    {
        type            calculated;
        value           uniform 0.1;
    }
    leftHand
    {
        type            calculated;
        value           uniform 0.1;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.1;
    }
    inlet
    {
        type            calculated;
        value           uniform 0.1;
    }
    innerRadius
    {
        type            calculated;
        value           uniform 0.1;
    }
}


